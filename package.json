{
  "name": "stack-analyze",
  "version": "1.2.6",
  "type": "module",
  "description": "cli tech stack analyze and pagespeed with node.js using the wappalyzer module. with google pagespeed api, hardware and crypto market",
  "main": "index.mjs",
  "bin": {
    "stack-analyze": "cli.js"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "boxen": "^7.1.1",
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "console-table-printer": "^2.11.2",
    "figlet": "^1.6.0",
    "gauge": "^5.0.1",
    "inquirer": "^9.2.10",
    "systeminformation": "^5.21.3",
    "timeago.js": "^4.0.2",
    "wappalyzer": "^6.10.63"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "gh-pages": "^6.0.0",
    "jsdoc": "^4.0.2"
  },
  "scripts": {
    "start": "node cli.js",
    "test": "node --test test/index.test.js",
    "lint:test": "eslint . --ext .js,.cjs,.mjs",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs --fix",
    "docs": "jsdoc -c jsdoc.json",
    "docs:deploy": "npm run docs && gh-pages -d docs"
  },
