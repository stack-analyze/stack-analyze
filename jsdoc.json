{
    "source": {
        "include": ["./functions/", "./cli.js", "./about.js", "./menu.js", "./utils.js" ],
        "includePattern": ".+\\.(c?js(doc|x)?)$",
        "excludePattern": "(node_modules|docs|test)"
    },
    "templates": {
    "default": {
        "name": "stack-analyze docs",
        "footerText": "stack-analyze is created and maintainer for omega5300",
        "staticFiles": {
            "include": ["./logo-module.webp"]
        }
    }
},
    "opts": {
        "recurse": false,
        "destination": "./docs",
        "readme": "./readme.md"
    }
}
